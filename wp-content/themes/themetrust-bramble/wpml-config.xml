		<custom-type translate="1">project</custom-type>
		<custom-type translate="1">testimonial</custom-type>
	</custom-types>
	<taxonomies>
		<taxonomy translate="1">skill</taxonomy>
	</taxonomies>
	<custom-fields>
		<custom-field action="translate">_bramble_header_options</custom-field>
		<custom-field action="translate">_bramble_header_hide</custom-field>
		<custom-field action="translate">_bramble_header_transparent_bg</custom-field>
